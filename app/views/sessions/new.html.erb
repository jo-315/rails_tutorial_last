<h1>Log in!!!!</h1>

<div class="raw">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for :session  do |f| %>
           
           <%= f.label :email, 'email' %>
           <%= f.email_field :email , class: 'form-control'%>
           
           <%= f.label :password %>
           <%= f.password_field :password, class: 'form-control' %>

           <%= f.submit 'Log in', class: "btn btn-primary" %>
        <% end %>
    </div>
</div>